<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewModels1="clr-namespace:Sample.ZebraBluetoothPrinter.Core.ViewModels;assembly=Sample.ZebraBluetoothPrinter.Core"
             x:Class="Sample.ZebraBluetoothPrinter.Core.Views.MainView">
  <ContentPage.BindingContext>
    <viewModels1:MainViewModel />
  </ContentPage.BindingContext>
  <StackLayout>
    <Label Text="Zebra printer test"></Label>
    <Button Text="Print test page" Command="{Binding PrintCommand}" HeightRequest="40"></Button>
  </StackLayout>
</ContentPage>