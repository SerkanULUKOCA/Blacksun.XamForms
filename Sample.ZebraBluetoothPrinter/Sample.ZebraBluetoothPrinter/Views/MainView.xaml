<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:controls="clr-namespace:Blacksun.XamForms.Controls;assembly=Blacksun.XamForms"
             xmlns:viewModels="clr-namespace:Sample.ZebraBluetoothPrinter.ViewModels;assembly=Sample.ZebraBluetoothPrinter"
             x:Class="Sample.ZebraBluetoothPrinter.Views.MainView">
  <ContentPage.BindingContext>
    <viewModels:MainViewModel />
  </ContentPage.BindingContext>
  <controls:DataForm >
    <controls:DataFormGroup Header="Zebra printer test">
      <controls:DataFormButton Text="Print Test Page" Command="{Binding PrintCommand}" HeightRequest="40"></controls:DataFormButton>
    </controls:DataFormGroup>
  </controls:DataForm>
</ContentPage>